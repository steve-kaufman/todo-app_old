    "config": "config/"
  },
  "engines": {
    "node": "^12.0.0",
    "yarn": ">= 0.18.0"
  },
  "scripts": {
    "test": "yarn run eslint && NODE_ENV= yarn run jest",
    "eslint": "eslint backend/. test/. --config .eslintrc.json",
